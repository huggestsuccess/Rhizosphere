{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints":{
      "Https": {
        "Url": "https://n-pecora.dev:443",
        "Path":"/etc/letsencrypt/live/n-pecora.dev"
        ///etc/letsencrypt/live/n-pecora.dev/fullchain.pem
      }
    },
    "Limits": {
      "MaxConcurrentConnections": 100,
      "MaxConcurrentUpgradedConnections": 100
    },
    "DisableStringReuse": true
  }
}
